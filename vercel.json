{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "airports/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "aircraft/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/create-checkout-session",
      "methods": ["POST"],
      "dest": "server.js"
    },
    {
      "src": "/session/(.*)",
      "methods": ["GET"],
      "dest": "server.js"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/airports/([^/]+)\\.html",
      "dest": "/airports/$1.html"
    },
    {
      "src": "/aircraft/([^/]+)\\.html",
      "dest": "/aircraft/$1.html"
    },
    {
      "src": "/success.html",
      "dest": "/success.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
